// This file is a "Hello, world!" in C++ language by GCC for wandbox.
#define _CRT_SECURE_NO_WARNINGS
#include <iostream>

using namespace std;

int N,temp1=0,temp2=0, Min=0, Max=0;

int Var1()
{
    for(int k=0;temp1<=N;k++)
    {
    temp1=temp1+k;
    }
    return temp1;
}

int Var2()
{
    for(int j=1;temp2<=N;j++)
    {
    temp2=temp2+j;
    }
    return temp2;
}


int main()
{
    scanf("%d", &N);
    Min=2+6*Var1;
    Max=1+6*Var2;
    
    if(N>=Min && N<=Max)
    {
        cout<<k;
    }
    
    
    return 0;
}
